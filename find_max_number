#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int max_of_four(int a, int b, int c, int d) {
    int max_value = a;
    if (b > max_value) {
        max_value = b;
    }
    if (c > max_value) {
        max_value = c;
    }
    if (d > max_value) {
        max_value = d;
    }
    return max_value;
}  

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    cout << max_of_four(a, b, c, d) << endl;
    
    return 0;
}
